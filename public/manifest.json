{
    "manifest_version": 3,
    "name": "QuickChange",
    "version": "1.0.0",
    "description": "A chrome extension for bg colour change",  
    "icons": {
      "32": "image32.png",
      "48": "image32.png",
      "128": "image32.png"
    },
    "action":{
      "default_popup": "index.html"
    },
    "permissions":[
      "scripting"
    ],
    "host_permissions":[
      "http://*/*",
       "https://*/*"
    ]
  }
  